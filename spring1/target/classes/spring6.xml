<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="coder" class="com.sk.spring6.autowire.Coder" scope="singleton" autowire="byType">
<property name="code" value="100"></property>
</bean>

<bean id="lap" class="com.sk.spring6.autowire.Laptop" primary="true"></bean>

<bean id="desk" class="com.sk.spring6.autowire.Desktop"></bean>

</beans>
